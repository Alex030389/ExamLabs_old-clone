                                        span *
                                input(type="text" placeholder="2018-12-13").modal-vacancy__input
                        .modal-vacancy__line-supplier-right
                            label.modal-vacancy__label
                                .modal-vacancy__title
                                    h4 Number of questions
                                        span *
                                input(type="text" placeholder="14").modal-vacancy__input
                    .modal-vacancy__add-file.modal-vacancy__add-file--padding
                        .button-add-file
