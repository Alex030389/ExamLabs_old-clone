                                            a(href="#").trainig-course__block-link
                                                .trainig-course__block-pic.trainig-course__block-pic--index-page
                                                    img(src="./assets/img/training-courses.png" data-rjs="3").trainig-course__block-img
                                                    .trainig-course__price
