                    <div class="search__title">
                        <h1>No question left unanswered. No bug left unsolved.</h1>
                    </div>
                </div>
            </div>
            <div class="questions">
                <div class="container container--width-faq">
                    <div class="questions__blocks">
                        <a class="questions__link questions__link--new" href="#general-block">
                            <div class="questions__blocks-wrapper-new">
                                <div class="questions__blocks-pic">
                                    <div class="questions__blocks-image">
                                        <img src="/assets/img/icon-general-questions.svg">
                                    </div>
                                </div>
                                <div class="questions__blocks-text questions__blocks-text--padding">
                                    <h3>General <br> Questions</h3>
                                </div>
                            </div>
                        </a>
                        <a class="questions__link questions__link--new" href="#vce-block">
                            <div class="questions__blocks-wrapper-new">
